html, body
  height 100%
  background-color rgb(76, 76, 74)
  font-family "儷黑 Pro", "LiHei Pro", "微軟正黑體", "Microsoft Jhenghei"
.container
  width 960px
  margin 0 auto
#header
  padding 105px 0 30px 60px
#logo
  margin 0 auto
  background-image url("/images/logo.png")
  width 520px
  height 139px
  position relative
  z-index 999
.member-avatar
  cursor pointer
.member-avatar, .member-meta1, .member-meta2
  display block
  width 150px
  height 150px
  border-radius 75px
  background-color #DDD
  position relative
  z-index 10
.member-meta1
  z-index 9
  top -150px
  > span
    width 150px
    text-align center
    display inline-block
    letter-spacing 0.25em
    margin-left 4px
    &.circle-name
      margin-top 54px
      font-size 1.5em
    &.circle-title
      margin-top 9px
      font-size 0.75em
.member-meta2
  cursor url(/images/zoomin.png), pointer
  z-index 8
  top -300px
  > span
    display inline-block
    margin 69px 0 0 30px
    font-size 1em
.profile-draft
  width 100%
  height 100%
  display block
  background-repeat no-repeat
  background-position 30px 23px
  opacity 1
  -ms-transition all 0.3s ease-out
  -o-transition all 0.3s ease-out
  -moz-transition all 0.3s ease-out
  -webkit-transition all 0.3s ease-out
  transition all 0.3s ease-out
#members
  list-style none
  margin-top 100px
  width 100%
  > li
    float left
    margin 0 20px
  [data-member='chaiyupai']
    > .member-avatar
      background-image url("/images/avatar/avatar_pa.jpg")
    > .member-meta1, .member-meta2
      background-color #1abc9c
    .profile-draft
      background-image url("/images/profile/profile_pa.png")
  [data-member='eric']
    > .member-avatar
      background-image url("/images/avatar/avatar_en.jpg")
    > .member-meta1, .member-meta2
      background-color #ecf0f1
    .profile-draft
      background-image url("/images/profile/profile_en.png")
  [data-member='blue']
    > .member-avatar
      background-image url("/images/avatar/avatar_bs.jpg")
    > .member-meta1, .member-meta2
      background-color #3498db
    .profile-draft
      background-image url("/images/profile/profile_bs.png")
  [data-member='gs']
    > .member-avatar
      background-image url("/images/avatar/avatar_gs.jpg")
    > .member-meta1, .member-meta2
      background-color #e67e22
    .profile-draft
      background-image url("/images/profile/profile_gs.png")
  [data-member='yswu']
    > .member-avatar
      background-image url("/images/avatar/avatar_ys.jpg")
    > .member-meta1, .member-meta2
      background-color #e78075
    .profile-draft
      background-image url("/images/profile/profile_ys.png")
  > li:hover:not(.animate-step1), > li:active:not(.animate-step1)
    > .member-meta1
      -ms-animation circle_meta1 0.3s linear 1
      -o-animation circle_meta1 0.3s linear 1
      -moz-animation circle_meta1 0.3s linear 1
      -webkit-animation circle_meta1 0.3s linear 1
      animation circle_meta1 0.3s linear 1
li.animate-step1
  > .member-meta1
    -ms-transform translateY(140px)
    -o-transform translateY(140px)
    -moz-transform translateY(140px)
    -webkit-transform translateY(140px)
    transform translateY(140px)
  > .member-meta2
    top -160px
    -ms-animation circle_meta2 0.3s linear 1
    -o-animation circle_meta2 0.3s linear 1
    -moz-animation circle_meta2 0.3s linear 1
    -webkit-animation circle_meta2 0.3s linear 1
    animation circle_meta2 0.3s linear 1
    -ms-animation-delay 0.05s
    -o-animation-delay 0.05s
    -moz-animation-delay 0.05s
    -webkit-animation-delay 0.05s
    animation-delay 0.05s
li.animate-step2
  > .member-meta2
    -ms-transform translateY(140px)
    -o-transform translateY(140px)
    -moz-transform translateY(140px)
    -webkit-transform translateY(140px)
    transform translateY(140px)
    -moz-transition none
    -webkit-transition none
    -o-transition none
    -ms-transition none
    transition none
    &.active
      // 顯示資訊過場
      -ms-transition all 0.3s ease-out
      -o-transition all 0.3s ease-out
      -moz-transition all 0.3s ease-out
      -webkit-transition all 0.3s ease-out
      transition all 0.3s ease-out
      > .profile-draft
        opacity 0
li.animate-step3
